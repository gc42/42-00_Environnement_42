#include <unistd.h>

int		fonction(int i)
{
	if (i < 5)
	{
		i += 1;
		write(1, "D", 1);
		fonction(i);
		write(1, "F", 1);
		return (0);
	}
	else
	{
		return (0);
	}
}

int		main()
{
	fonction(0);
	return (0);
}
